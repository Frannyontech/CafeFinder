<% if user_signed_in? %>
    <%= current_user.email %>
        <%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
        <%= link_to 'Account Settings', edit_user_registration_path %>
<% else %>
    <%= link_to 'Log In', new_user_session_path %>
    <%= link_to 'Sign In', new_user_registration_path %>
<% end %>